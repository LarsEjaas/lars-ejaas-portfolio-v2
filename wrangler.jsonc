{
  "main": "dist/_worker.js/index.js",
  "name": "my-astro-app",
  // Update to today's date
  "compatibility_date": "2026-01-31",
  "compatibility_flags": [
    "nodejs_compat",
    "enable_nodejs_process_v2",
    "global_fetch_strictly_public",
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist",
  },
  "observability": {
    "enabled": true,
  },
  "$schema": "./node_modules/wrangler/config-schema.json",
}
